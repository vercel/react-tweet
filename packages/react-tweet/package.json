{
  "name": "react-tweet",
  "version": "2.0.0",
  "repository": "https://github.com/vercel-labs/react-tweet.git",
  "author": "Luis Alvarez (https://twitter.com/luis_fades)",
  "scripts": {
    "build": "pnpm build:swc && pnpm types",
    "build:swc": "swc src -d dist --copy-files",
    "dev": "pnpm build:swc -w",
    "types": "tsc --emitDeclarationOnly",
    "lint": "TIMING=1 eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf dist && rm -rf .turbo"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vercel-labs/react-tweet/issues"
  },
  "sideEffects": [
    "./dist/tweet-container.js"
  ],
  "type": "module",
  "exports": {
    ".": {
      "react-server": "./dist/index.js",
      "default": "./dist/index.client.js"
    },
    "./api": "./dist/api/index.js",
    "./theme.css": "./dist/theme.css"
  },
  "files": [
    "dist/**/*.{js,d.ts,css}"
  ],
  "typesVersions": {
    "*": {
      "index": [
        "src/index"
      ],
      "api": [
        "src/api/index"
      ],
      "*": []
    }
  },
  "publishConfig": {
    "access": "public",
    "typesVersions": {
      "*": {
        "index": [
          "dist/index.d.ts"
        ],
        "api": [
          "dist/api/index.d.ts"
        ],
        "*": []
      }
    }
  },
  "peerDependencies": {
    "react": ">= 18.0.0",
    "react-dom": ">= 18.0.0"
  },
  "dependencies": {
    "@swc/helpers": "^0.4.14",
    "clsx": "^1.2.1",
    "date-fns": "^2.9.0",
    "swr": "^2.1.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.36",
    "@types/node": "18.13.0",
    "@types/react": "^18.0.27",
    "chokidar": "^3.5.3",
    "eslint": "^8.34.0",
    "eslint-config-next": "^13.2.3",
    "typescript": "^4.9.5"
  }
}
