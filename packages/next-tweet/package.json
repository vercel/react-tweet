{
  "name": "next-tweet",
  "version": "0.0.1",
  "repository": "https://github.com/lfades/next-tweet.git",
  "author": "Luis Alvarez (https://twitter.com/luis_fades)",
  "scripts": {
    "lint": "TIMING=1 eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf .turbo"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lfades/next-tweet/issues"
  },
  "exports": {
    ".": "./src/index.ts",
    "./dark.module.css": "./src/dark.module.css",
    "./light.module.css": "./src/light.module.css",
    "./*": [
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts"
    ]
  },
  "files": [
    "src"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "peerDependencies": {
    "next": ">= 13.0.0",
    "react": ">= 18.0.0",
    "react-dom": ">= 18.0.0"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "date-fns": "^2.9.0"
  },
  "devDependencies": {
    "@types/node": "18.13.0",
    "@types/react": "^18.0.27",
    "eslint": "^8.34.0",
    "typescript": "^4.9.5"
  }
}
